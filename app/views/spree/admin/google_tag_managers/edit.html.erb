<% content_for :page_title do %>
  <%= Spree.t(:google_tag_manager) %>
<% end %>
<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @google_tag_manager } %>
<%= form_for @google_tag_manager, url: admin_google_tag_managers_path do |f| %>
  <fieldset>
     <%= render partial: 'form', locals: { f: f } %>
     <div class="form-actions" data-hook="buttons">
        <%= button Spree.t('actions.update'), 'save.svg', 'submit', {class: 'btn-success', data: { disable_with: "#{ Spree.t(:saving) }..." }} %>
     </div>
  </fieldset>
<% end %>
